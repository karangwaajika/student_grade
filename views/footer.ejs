<script type="text/javascript">
  $(document).ready(function(){
       $('#add').click(function(){
        $('#econ').toggleClass('fa fa-eye-slash');
        var input = $("#password");
        if (input.attr("type")==="password") {
          input.attr("type","text");
        }
        else{
          $('#econ').toggleClass('fa fa-eye');
          input.attr("type","password");
        }
       });
     });
</script>

 
